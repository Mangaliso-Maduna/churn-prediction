<% layout('layouts/boilerplate') %> 
    <h1>This page show all bank customers</h1>
    <ul>
        <% for( let customer of customers ) { %>
            <li>
                <a href="/customers/<%= customer._id %>"><%= customer.name %></a>
            </li>
        <% } %>
    </ul>
    <a href="/customers/new">Add new customer</a>
